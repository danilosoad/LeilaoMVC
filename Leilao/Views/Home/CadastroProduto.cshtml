
@{
    ViewData["Title"] = "CadastroProduto";
}

<h1>Cadastro Produto</h1>
<form>
    <div class="form-group">
        <label for="nomeProduto">Nome</label>
        <input type="text" class="form-control" id="nomeProduto" aria-describedby="valorhelp" name="NOME" placeholder="Nome">
    </div>
    <div class="form-group">
        <label for="valorProduto">Valor</label>
        <input type="text" class="form-control" id="valorProduto" aria-describedby="valorhelp" name="VALOR" placeholder="Valor">
    </div>
    <button asp-controller="Home" asp-action="CadastrarProduto" type="submit" class="btn btn-primary">Enviar</button>
</form>


